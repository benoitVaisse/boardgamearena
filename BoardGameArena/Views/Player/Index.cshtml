@model IEnumerable<BoardGameArena.Models.Player>

@{
    ViewData["Title"] = "Joueurs";
}

<h1>Nos Joueurs</h1>

@if (Model.Count() > 0)
{
    <div class="row">

    @foreach(Player Player in Model)
    {
        <partial name="~/Views/Player/_partials/OnePlayer.cshtml" model="@Player" />
    }

    </div>

}
